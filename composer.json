{
    "name": "Olive_James_Bond/google-translate-php",
    "description": "Free Google Translate API PHP Package",
    "keywords": ["google", "translate", "translator", "php"],
    "license": "MIT",
    "homepage": "http://github.com/Olive-James-Bond/Trans",
